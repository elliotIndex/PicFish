    "url": "https://github.com/elliotaplant/PicFish/issues"
  },
  "homepage": "https://github.com/elliotaplant/PicFish#readme",
  "dependencies": {
    "express": "^4.14.0",
    "jsdom": "^9.9.1",
    "knuth-shuffle": "^1.0.1",
    "mongodb": "^2.2.19",
    "mu2": "^0.5.21",
    "node-cron": "^1.1.2",
    "parse5": "^3.0.1",
    "request": "^2.79.0"
  }
}
